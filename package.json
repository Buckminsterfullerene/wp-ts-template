{
  "name": "wp-ts-template",
  "version": "0.0.1",
  "description": "Webpack Typscript Template",
  "scripts": {
    "start": "webpack-dev-server --port 4110 --config webpack.config.js --inline --watch --progress --profile --colors --content-base src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "dev": "webpack --mode development",
    "production": "webpack --mode production",
    "test": "karma start karma.config.js",
    "wp_stats": "npm run build_stats && npx webpack-bundle-analyzer dist/wp-ts-template/stats.json",
    "src_map_stats": "npx source-map-explorer dist/wp-ts-template/main.js",
    "build_stats": "ng build --prod --stats-json --output-hashing none --aot --configuration production --extract-css true --source-map --output-path dist/wp-ts-template/"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "Buckminsterfullerene",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "@types/node": "^10.12.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.10",
    "fs-extra": "^7.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "jquery": "^3.3.1",
    "karma": "^3.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.5",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.3",
    "source-map-explorer": "^1.6.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@types/jquery": "^3.3.20"
  }
}
