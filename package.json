{
  "name": "wp-ts-template",
  "version": "0.0.1",
  "description": "Webpack Typscript Template",
  "scripts": {
    "start": "webpack-dev-server --port 4120 --config webpack.config.js --inline --watch --progress --profile --colors --content-base src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "dev": "webpack --mode development",
    "production": "webpack --mode production",
    "test": "karma start karma.config.js",
    "wp_stats": "npm run build_stats && npx webpack-bundle-analyzer dist/wp-ts-template/stats.json",
    "src_map_stats": "npx source-map-explorer dist/wp-ts-template/main.js",
    "build_stats": "ng build --prod --stats-json --output-hashing none --aot --configuration production --extract-css true --source-map --output-path dist/wp-ts-template/"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "Buckminsterfullerene",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^3.6.9",
    "@types/node": "^14.14.37",
    "autoprefixer": "^10.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "fs-extra": "^9.1.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "image-webpack-loader": "^7.0.1",
    "jasmine-core": "^3.7.1",
    "jquery": "^3.6.0",
    "karma": "^6.3.14",
    "karma-jasmine": "^4.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^5.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "^5.0.0",
    "postcss-loader": "^5.2.0",
    "sass-loader": "^11.0.1",
    "source-map-explorer": "^2.5.2",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@types/jquery": "^3.5.5"
  }
}
